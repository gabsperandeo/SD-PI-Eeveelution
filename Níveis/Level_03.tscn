[sub_resource type="Shader" id=27]
code = "shader_type canvas_item;

uniform sampler2D color_texture;

void fragment()
{
	vec2 data = texture(SCREEN_TEXTURE, SCREEN_UV).rg;
	if(data.g > 0.0)
	{
		discard;
	}
	
	COLOR = texture(color_texture, vec2(data.r, 0.0));
}"

