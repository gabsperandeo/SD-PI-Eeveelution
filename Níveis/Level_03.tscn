script/source = "extends Node2D

export var finished : bool
export var next_scene : String
export var reset_var : bool

func _ready():
	finished = false
	next_scene = \"res://Níveis/Level_04.tscn\"

func reset():
	reset_var = true

func _process(delta):
	if $Doors.finished:
		finished = true
	else:
		finished = false
